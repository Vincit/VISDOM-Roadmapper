{
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^7.2.0",
    "nyc": "^15.0.1"
  },
  "name": "server",
  "version": "0.1",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "migrate": "knex --knexfile=./knexfile.ts migrate:latest",
    "seed-db": "knex --knexfile=./knexfile.ts seed:run",
    "start": "knex --knexfile=./knexfile.ts migrate:latest && knex --knexfile=./knexfile.ts seed:run && npx ts-node ./src/server.ts",
    "start-db": "cd .. && docker-compose -f docker-compose.yml up -d roadmapper-database",
    "test": "mocha -r ts-node/register tests/**/*.test.ts --timeout 10000",
    "start-prod": "tsc && knex --knexfile=./knexfile.ts migrate:latest && knex --knexfile=./knexfile.ts seed:run && node ./build/src/server.js"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^8.0.8",
    "@types/jira-client": "^6.13.1",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-session": "^5.10.2",
    "@types/koa__cors": "^3.0.1",
    "@types/koa__router": "^8.0.2",
    "@types/node-rsa": "^1.0.0",
    "@types/passport-auth-token": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/uuid": "^8.3.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "jira-client": "^6.21.1",
    "jira-connector": "^3.1.0",
    "knex": "^0.21.1",
    "koa": "^2.12.0",
    "koa-bodyparser": "^4.3.0",
    "koa-passport": "^4.1.3",
    "koa-session": "^6.0.0",
    "node-rsa": "^1.1.1",
    "objection": "^2.1.4",
    "objection-password": "^2.0.0",
    "passport-auth-token": "^1.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  }
}
